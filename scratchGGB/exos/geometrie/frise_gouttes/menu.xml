<?xml version="1.0"?>
<xml id="toolbox" style="display: none">
    <category name="Questions" id="demos" colour="#FFFF00" secondaryColour="#FFFF00">
			<button text="1) Translations" callbackKey="demo11" prog="exos/geometrie/frise_gouttes/q1_prog.xml" geo="exos/geometrie/frise_gouttes/q1_geogebra.ggb">
<![CDATA[
a) Construis la goutte 3 par translation de la goutte 2
b) Construis la goutte 4 par translation de la goutte 3
]]>
			</button>
			<button text="2) Sous-programmes" callbackKey="demo12" prog="exos/geometrie/frise_gouttes/q2_prog.xml" geo="exos/geometrie/frise_gouttes/q2_geogebra.ggb">
<![CDATA[
